{
  "name": "zebra-storm",
  "description": "zebra-storm plugin module manages stormflash enabled zebra instance",
  "version": "0.1.0",
  "author": {
    "name": "Suresh Kumar"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stormstack/quagga-storm"
  },
  "dependencies": {
    "async": ">=0.9.0",
    "fmerge": "1.1.0",
    "mkdirp": ">=0.5.0",
    "stormdata": ">=1.2.0",
    "stormregistry": "1.2.x",
    "stormservice": ">=1.0.2"
  },
  "engines": {
    "node": ">=0.6.x"
  },
  "main": "./lib/zebra-service",
  "stormflash": {
    "packages": [
      "apt-get://quagga"    
    ],
    "services": [
      "routing.*"
    ],
    "plugin": "lib/plugin"
  },
  "config": {
    "logfile": "/var/log/quagga-zebra-storm.log",
    "storm": {
      "plugins": [
        "lib/plugin"
      ]
    }
  },
  "devDependencies": {
    "coffee-script": ">=1.7"
  },
  "optionalDependencies": {},
  "keywords": [
    "quagga",
    "zebra"
  ],
  "scripts": {
    "prepublish": "mkdir -p lib; coffee -o lib -c src"
  },
  "contributors": ["Sivaprasath Busa"],
  "readmeFilename": "README.md",
  "readme": "",
  "_id": "quagga-storm@0.1.0",
  "dist": {    
  },
  "_from": "quagga-storm@*",
  "_resolved": ""
}
