{
  "name": "quagga-storm",
  "description": "quagga-storm plugin module manages stormflash enabled zebra and its associated routing protocols like ospf, rip and bgp",
  "version": "0.7.3",
  "author": {
    "name": "Suresh Kumar"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stormstack/quagga-storm"
  },
  "dependencies": {
    "async": "0.9.0",
    "fmerge": "1.1.0",
    "mkdirp": "0.5.0",
    "stormdata": "1.2.2",
    "stormregistry": "1.2.1",
    "stormservice": "1.0.5"
  },
  "engines": {
    "node": ">=0.6.x"
  },
  "main": "./lib/zebra-service",
  "stormflash": {
    "packages": [
      "apt-get://quagga"    
    ],
    "services": [
      "routing.*"
    ],
    "plugin": "lib/plugin"
  },
  "config": {
    "logfile": "/var/log/quagga-storm.log",
    "storm": {
      "plugins": [
        "lib/plugin"
      ]
    }
  },
  "devDependencies": {
    "coffee-script": ">=1.7"
  },
  "optionalDependencies": {},
  "keywords": [
    "quagga",
    "zebra"
  ],
  "contributors": [
      "Sivaprasath Busa"
  ],
  "readmeFilename": "README.md",
  "readme": "",
  "_id": "quagga-storm@0.1.0",
  "dist": {    
  },
  "_from": "quagga-storm@*",
  "_resolved": ""
}
